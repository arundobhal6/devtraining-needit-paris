      return;
   }

   //Type appropriate comment here, and begin script below
   if(newValue>=18){
	   g_form.addInfoMessage('Now you can vote');
	   var c=g_form.getDecimalValue('u_integer_2');
	   alert("Value is "+c);
	alert("The Integer value is "+g_form.getIntValue('u_integer_2'));
	 
   }
	else{
		   g_form.clearOptions('u_choice_5');
	   g_form.addOption('u_choice_4','5','Dean');
		g_form.addErrorMessage('You cannot vote');
		
	}
}
