        <name>OnChange</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
   if (isLoading || newValue === '') {
      return;
   }
	
   //Type appropriate comment here, and begin script below
	if(g_form.getValue('short_description')=='Hello'){
		g_form.flash("priority",'color-red',0);
		alert("Value is Hello");
		
	}
	else
		{
			alert("Some other Value"+g_form.getValue('short_description'));
		}
	alert(g_form.addDecoration('short_description','icon-star','Mark as favourite','color-green'));
	
   
}]]></script>
